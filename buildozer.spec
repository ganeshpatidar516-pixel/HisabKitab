[app]
title = HisabKitab AI Ultra
package.name = hisabkitabai
package.domain = com.ganesh.ultra
source.dir = .
source.include_exts = py,png,jpg,kv,atlas,html,js,css
version = 1.0.0
requirements = python3,kivy,requests,certifi,pydantic
source.exclude_dirs = tests, bin, venv, aiapi, .buildozer, .git, __pycache__
icon.filename = logo.png
orientation = portrait
fullscreen = 0
android.permissions = INTERNET
android.archs = arm64-v8a
